<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_main" tools:context=".MainActivity"
    android:background="@drawable/f30">


    <TextView android:text="@string/Hello_world"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv_hello_world"
        android:gravity="center"
        android:layout_alignParentTop="true" />


    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageView6"
        android:src="@drawable/ca1"
        android:onClick="btnHwAct"
        android:layout_alignTop="@+id/imageView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="37dp" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="160dp"
        android:id="@+id/imageView8"
        android:src="@drawable/e4"
        android:onClick="onclickbutton"
        android:layout_below="@+id/imageView10"
        android:layout_alignLeft="@+id/imageView9"
        android:layout_alignStart="@+id/imageView9" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="175dp"
        android:id="@+id/imageView9"
        android:src="@drawable/e2"
        android:onClick="tnSdAct"
        android:layout_below="@+id/tv_hello_world"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="140dp"
        android:id="@+id/imageView10"
        android:src="@drawable/e3"
        android:onClick="surleclick"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView9" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/textmi"
        android:src="@drawable/c6"
        android:onClick="hbibi"
        android:nestedScrollingEnabled="false"
        android:layout_alignBottom="@+id/imageView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageView"
        android:src="@drawable/plus1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView10"
        android:onClick="hna"
        android:layout_alignTop="@+id/imageView10"
        android:layout_above="@+id/imageView8"
        android:layout_toRightOf="@+id/imageView10" />


</RelativeLayout>